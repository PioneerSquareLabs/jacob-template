{
  "language": "TypeScript",
  "installCommand": "npm install",
  "formatCommand": "npm run lint:fix",
  "buildCommand": "SKIP_ENV_VALIDATION=1 npm run build --verbose",
  "style": "Tailwind",
  "iconSet": "Font Awesome",
  "directories": {
    "components": "/src/components",
    "pages": "/src/pages",
    "types": "/src/types",
    "styles": "/src/styles",
    "tailwindConfig": "/tailwind.config.ts",
    "staticAssets": "/public"
  },
  "stateManagement": {
    "tool": "None"
  },
  "testing": {
    "writeTests": false
  },
  "storybook": {
    "writeStories": true,
    "storiesLocation": "/src/stories"
  },
  "envVariables": {
    "exampleFile": "/.env.example"
  },
  "env": ""
}
